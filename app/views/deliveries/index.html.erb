<p id="notice"><%= notice %></p>

<h1>Deliveries</h1>

<table>
  <thead>
    <tr>
      <th>Course number</th>
      <th>Course</th>
      <th>Credits</th>
      <th>Fees</th>
      <th>Seat load</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Location</th>
      <th>Requestor</th>
      <th>Poc</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deliveries.each do |delivery| %>
      <tr>
        <td><%= delivery.course_number %></td>
        <td><%= delivery.course_id %></td>
        <td><%= delivery.credits %></td>
        <td><%= delivery.fees %></td>
        <td><%= delivery.seat_load %></td>
        <td><%= delivery.start_date %></td>
        <td><%= delivery.end_date %></td>
        <td><%= delivery.start_time %></td>
        <td><%= delivery.end_time %></td>
        <td><%= delivery.location %></td>
        <td><%= delivery.requestor %></td>
        <td><%= delivery.poc %></td>
        <td><%= delivery.notes %></td>
        <td><%= link_to 'Show', delivery %></td>
        <td><%= link_to 'Edit', edit_delivery_path(delivery) %></td>
        <td><%= link_to 'Destroy', delivery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delivery', new_delivery_path %>
