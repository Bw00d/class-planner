<p id="notice"><%= notice %></p>

<h1>Unit Instructors</h1>

<table>
  <thead>
    <tr>
      <th>Delivery</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unit_instructors.each do |unit_instructor| %>
      <tr>
        <td><%= unit_instructor.delivery_id %></td>
        <td><%= unit_instructor.user_id %></td>
        <td><%= link_to 'Show', unit_instructor %></td>
        <td><%= link_to 'Edit', edit_unit_instructor_path(unit_instructor) %></td>
        <td><%= link_to 'Destroy', unit_instructor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unit Instructor', new_unit_instructor_path %>
